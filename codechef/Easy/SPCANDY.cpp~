#include<iostream>
#include<stdio.h>
#include<math.h>
#include<algorithm>
#include<string.h>
using namespace std;
int main()
{
   	int t;  scanf("%d",&t);
    long long int k,n,c,t;
    while(t>0)
    { 
		scanf("%lld%lld",&n,&k);
        if(k==0)
        {
           c=0;t=n;
        }
		else 
        {
            c=n/k; t=n%k;
        }
        printf("%lld %lld\n",c,t);        
        t--;
    }
	return 0;
}
